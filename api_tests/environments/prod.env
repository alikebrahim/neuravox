# Production Environment
base_url=https://api.neuravox.com/api/v1
host=api.neuravox.com
api_version=v1

# Authentication
user_id=prod-test-user
key_name=prod-test-key

# Logging
log_level=WARN
verbose=false

# Production Overrides
rate_limit_per_minute=30
timeout_seconds=180
test_file_path=./test-data/sample.wav

# Safety settings for production
dry_run=true
skip_destructive_tests=true